<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="600.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="view.HomePage">
   <children>
      <ImageView fitHeight="600.0" fitWidth="800.0">
         <image>
            <Image url="@../img/9.jpg" />
         </image>
      </ImageView>
      <Text fx:id="homeText" fill="WHITE" layoutX="190.0" layoutY="30.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Home">
         <font>
            <Font name="System Bold Italic" size="20.0" />
         </font>
      </Text>
      <Text fx:id="regText" layoutX="267.0" layoutY="30.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Registration">
         <font>
            <Font name="System Bold Italic" size="20.0" />
         </font>
      </Text>
      <Text fx:id="recipesText" layoutX="404.0" layoutY="30.0" smooth="false" strokeType="OUTSIDE" strokeWidth="0.0" text="Recipes">
         <font>
            <Font name="System Bold Italic" size="20.0" />
         </font>
      </Text>
      <ImageView fx:id="profileImg" fitHeight="30.0" fitWidth="34.0" layoutX="712.0" layoutY="7.0" onMouseClicked="#myProfile">
         <image>
            <Image url="@../img/icons8-nome-100.png" />
         </image>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </ImageView>
      <ImageView fx:id="logOutimg" fitHeight="30.0" fitWidth="28.0" layoutX="758.0" layoutY="7.0" onMouseClicked="#logOut">
         <image>
            <Image url="@../img/icons8-uscita-100.png" />
         </image>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </ImageView>
      <ImageView fx:id="note" fitHeight="39.0" fitWidth="38.0" layoutX="490.0" layoutY="3.0" onMouseClicked="#createNote">
         <image>
            <Image url="@../img/icons8-libretto-a-spirale-legato-80.png" />
         </image>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </ImageView>
      <Button fx:id="goButton" blendMode="MULTIPLY" cacheShape="false" centerShape="false" focusTraversable="false" layoutX="14.0" layoutY="530.0" mnemonicParsing="false" onAction="#go" prefHeight="53.0" prefWidth="50.0" scaleShape="false">
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
         <graphic>
            <ImageView fitHeight="55.0" fitWidth="55.0" pickOnBounds="true" smooth="false">
               <image>
                  <Image url="@../img/go.png" />
               </image>
            </ImageView>
         </graphic>
      </Button>
   </children>
</AnchorPane>
