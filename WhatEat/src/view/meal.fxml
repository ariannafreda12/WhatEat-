<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Cursor?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="600.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="view.MealPage">
   <children>
      <ImageView fx:id="Starters" cache="true" cacheHint="SCALE" fitHeight="600.0" fitWidth="800.0">
         <image>
            <Image url="@../img/3.jpg" />
         </image>
      </ImageView>
      <Text fx:id="txtStarters" layoutX="235.0" layoutY="367.0" onMouseClicked="#chooseStarters" strokeType="OUTSIDE" strokeWidth="0.0" text="Starters" underline="true">
         <font>
            <Font name="System Bold Italic" size="22.0" />
         </font>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </Text>
      <Text fx:id="txtMain" layoutX="360.0" layoutY="367.0" onMouseClicked="#chooseMainCourses" strokeType="OUTSIDE" strokeWidth="0.0" text="Main courses" underline="true">
         <font>
            <Font name="System Bold Italic" size="22.0" />
         </font>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </Text>
      <Text fx:id="txtSecond" layoutX="520.0" layoutY="367.0" onMouseClicked="#choosSecondCourses" strokeType="OUTSIDE" strokeWidth="0.0" text="Second courses" underline="true">
         <font>
            <Font name="System Bold Italic" size="22.0" />
         </font>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </Text>
      <Text fx:id="txtDessert" layoutX="546.0" layoutY="534.0" onMouseClicked="#chooseDessert" strokeType="OUTSIDE" strokeWidth="0.0" text="Desserts" underline="true">
         <font>
            <Font name="System Bold Italic" size="22.0" />
         </font>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </Text>
      <Text fx:id="txtBreakfast" layoutX="386.0" layoutY="534.0" onMouseClicked="#chooseBreakfast" strokeType="OUTSIDE" strokeWidth="0.0" text="Breakfast" underline="true">
         <font>
            <Font name="System Bold Italic" size="22.0" />
         </font>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </Text>
      <Text fx:id="txtSideDishes" layoutX="217.0" layoutY="534.0" onMouseClicked="#chooseSide" strokeType="OUTSIDE" strokeWidth="0.0" text="Side Dishes" underline="true">
         <font>
            <Font name="System Bold Italic" size="22.0" />
         </font>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </Text>
      <Text fx:id="regText" layoutX="267.0" layoutY="32.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Registration">
         <font>
            <Font name="System Bold Italic" size="22.0" />
         </font>
      </Text>
      <Text fx:id="recipesText" fill="WHITE" layoutX="415.0" layoutY="32.0" smooth="false" strokeType="OUTSIDE" strokeWidth="0.0" text="Recipes">
         <font>
            <Font name="System Bold Italic" size="22.0" />
         </font>
      </Text>
      <Text fx:id="homeText" layoutX="190.0" layoutY="32.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Home">
         <font>
            <Font name="System Bold Italic" size="22.0" />
         </font>
      </Text>
   </children>
</AnchorPane>
